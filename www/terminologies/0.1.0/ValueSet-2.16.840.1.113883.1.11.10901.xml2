  <description value="JDV_HL7_ParticipationType_CISIS"/>
  <compose>
    <include>
      <system value="http://terminology.hl7.org/CodeSystem/v3-ParticipationType"/>
      <concept>
        <code value="ADM"/>
        <display value="Responsable de l'admission"/>
      </concept>
      <concept>
        <code value="ALY"/>
        <display value="Cible d'une observation (substance ou composant le plus spécifique)"/>
      </concept>
      <concept>
        <code value="ATND"/>
        <display value="Superviseur / Responsable des soins"/>
      </concept>
      <concept>
        <code value="AUT"/>
        <display value="Auteur"/>
      </concept>
      <concept>
        <code value="AUTHEN"/>
        <display value="Valideur des résultats (ex : un biologiste ou un système expert)"/>
      </concept>
      <concept>
        <code value="BBY"/>
        <display value="Nouveau né"/>
      </concept>
      <concept>
        <code value="BEN"/>
        <display value="Bénéficiaire"/>
      </concept>
      <concept>
        <code value="CAGNT"/>
        <display value="Agent causal"/>
      </concept>
      <concept>
        <code value="CALLBCK"/>
        <display value="Contact à rappeler"/>
      </concept>
      <concept>
        <code value="CAT"/>
        <display value="Catalyseur"/>
      </concept>
      <concept>
        <code value="CON"/>
        <display value="Consultant"/>
      </concept>
      <concept>
        <code value="COV"/>
        <display value="Partie couverte (titulaire ou bénéficiaire)"/>
      </concept>
      <concept>
        <code value="CSM"/>
        <display value="Consommable"/>
      </concept>
